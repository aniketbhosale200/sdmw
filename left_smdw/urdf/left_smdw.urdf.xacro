  <!-- Define the left_smdw_wheel macro -->
  <xacro:macro name="left_smdw_wheel" params="prefix">
    <!-- Define properties -->
    <xacro:property name="mesh_path" value="file:///home/imbatman/smdw_ws/src/left_smdw/meshes/" />
    <xacro:property name="roller_mass" value="0.0343" />
