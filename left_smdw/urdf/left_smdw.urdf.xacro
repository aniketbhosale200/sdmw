        <geometry>
          <mesh filename="${mesh_path}FL_${type}_link_${number}.STL" />
        </geometry>
      </collision>
    </link>
    <joint name="${prefix}${type}_joint_${number}" type="continuous">
      <origin xyz="${xyz}" rpy="${rpy}" />
      <parent link="${prefix}base_link" />
      <child link="${prefix}${type}_link_${number}" />
      <axis xyz="${axis}" />
    </joint>

